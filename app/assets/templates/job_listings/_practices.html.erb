<div class='control-group'>
  <label class='control-label'>Practices</label>
  <div class='controls'>
    <table class='table table-bordered'>
      <tr ng-repeat="row in data.practices">
        <td ng-repeat="practice in row">
          <label class='checkbox'>
            <input type="checkbox" ng-model="practice.checked"\>{{practice.name}}
          </label>
        </td>
      </tr>
    </table>
  </div>
</div>
