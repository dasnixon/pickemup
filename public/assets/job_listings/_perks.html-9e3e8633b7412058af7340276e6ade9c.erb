<div class="form-group">
  <label class='col-sm-2 control-label'><strong>Perks</strong></label>
  <div class="col-sm-10">
    <table class="table table-bordered">
      <tr class="preference" ng-repeat="row in data.perks">
        <td class="preference-list" ng-click="toggleActiveHash(attributes)" ng-class="{active: attributes.checked}" ng-repeat="attributes in row">
          {{attributes.name}}
        </td>
      </tr>
    </table>
  </div>
</div>
