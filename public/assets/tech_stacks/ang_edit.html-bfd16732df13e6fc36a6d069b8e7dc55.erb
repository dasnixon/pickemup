<form class='form-horizontal techStack', name='techStackForm', ng-submit="update()">
  <div class='row row-offcanvas row-offcanvas-left'>
    <div class="col-md-2 sidebar-offcanvas" id="sidebar">
      <ul class='list-unstyled'>
        <li class="means-yes alert">
          This means yes or preferred
        </li>
        <li class="means-no alert">
          This means no or not preferred
        </li>
        <li ng-show="success.length > 0" class="alert alert-success">
          {{success}}
        </li>
        <li ng-show="dirty_message.length > 0" class="alert alert-info">
          {{dirty_message}}
        </li>
        <li ng-show="error_updating.length > 0" class="alert alert-danger">
          {{error_updating}}
        </li>
        <li ng-show="dirty_message.length > 0">
          <button class='btn btn-success' type="submit">Save Your Tech Stack</button>
        </li>
      </ul>
    </div>
    <div class="col-md-9-right">
      <div ng-include src="'/assets/tech_stacks/_tech_stack_form.html-ad408e3f32362c1d75f446b60b43618b.erb'"></div>
    </div>
  </div>
</form>
