<form class='tech_stack form-horizontal', name='techStackForm', ng-submit="create()">
  <div class="row-fluid">
    <div class="span2">
      <div class="span2 affix">
        <ul class="unstyled">
          <li class="means-yes alert">
            This means yes or preferred
          </li>
          <li class="means-no alert">
            This means no or not preferred
          </li>
          <li ng-show="success.length > 0" class="alert alert-success">
            {{success}}
          </li>
          <li ng-show="dirty_message.length > 0" class="alert alert-info">
            {{dirty_message}}
          </li>
          <li ng-show="error_updating.length > 0" class="alert alert-error">
            {{error_updating}}
          </li>
          <li ng-show="dirty_message.length > 0">
            <button class='btn btn-info' type="submit">Save Your Tech Stack</button>
          </li>
        </ul>
      </div>
    </div>
    <div class="span8 span-fixed-sidebar">
      <ng-include src="'/assets/tech_stacks/_tech_stack_form.html-0fb9ea0c7e738e06b43e29d7afe154c4.erb'" />
    </div>
  </div>
</form>
